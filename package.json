{
  "name": "duelnow-token",
  "version": "1.0.0",
  "description": "ERC20 token",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx husky install && npx npm-force-resolutions",
    "compile": "hardhat compile --force",
    "coverage": "npx hardhat coverage --solcoverjs ./.solcover.ts",
    "generate-types": "npx hardhat typechain",
    "test": "hardhat test",
    "deploy:localhost": "npx hardhat migrate --network localhost",
    "deploy:sepolia": "npx hardhat migrate --network sepolia --verify --confirmations 5",
    "deploy:arbitrumSepolia": "npx hardhat migrate --network arbitrumSepolia --verify --confirmations 5",
    "deploy:mainnet": "npx hardhat migrate --network mainnet --verify --confirmations 5",
    "deploy:arbitrumOne": "npx hardhat migrate --network arbitrumOne --verify --confirmations 5",
    "lint-fix": "npm run lint-sol-fix && npm run lint-ts-fix && npm run lint-json-fix",
    "lint-json-fix": "prettier --write \"./**/*.json\"",
    "lint-ts-fix": "prettier --write \"./**/*.ts\"",
    "lint-sol-fix": "prettier --write \"contracts/**/*.sol\""
  },
  "overrides": {
    "ws": "^8.16.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duelnow/token.git"
  },
  "keywords": [
    "duelnow",
    "duelNow",
    "dnow",
    "DNOW"
  ],
  "author": "DuelNow Engineering",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/duelnow/token/issues"
  },
  "homepage": "https://github.com/duelnow/token#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@solarity/hardhat-migrate": "^1.5.9",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.2",
    "chai": "^4.3.10",
    "dotenv-extended": "^2.9.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.17.3",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.6.2",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.5",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@chainlink/contracts": "0.8.0",
    "@openzeppelin/contracts": "4.9.3",
    "@solarity/solidity-lib": "2.6.5"
  }
}
